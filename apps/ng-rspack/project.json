{
  "name": "ng-rspack",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/ng-rspack/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rspack:rspack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "web",
        "rspackConfig": "rspack.config.js",
        "outputPath": "dist/ng-rspack/rspack",
        "main": "apps/ng-rspack/src/main.ts",
        "tsConfig": "apps/ng-rspack/tsconfig.app.json",
        "assets": [
          "apps/ng-rspack/src/favicon.ico",
          "apps/ng-rspack/src/assets"
        ]
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production",
          "optimization": true,
          "sourceMap": false
        }
      },
      "defaultConfiguration": "production"
    },
    "build-wp": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/ng-rspack/webpack",
        "index": "apps/ng-rspack/src/index.html",
        "main": "apps/ng-rspack/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/ng-rspack/tsconfig.app.json",
        "assets": [
          "apps/ng-rspack/src/favicon.ico",
          "apps/ng-rspack/src/assets"
        ],
        "styles": ["apps/ng-rspack/src/styles.css"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "outputHashing": "all",
          "extractLicenses": false
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "build-es": {
      "executor": "@nx/angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/ng-rspack/esbuild",
        "index": "apps/ng-rspack/src/index.html",
        "browser": "apps/ng-rspack/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/ng-rspack/tsconfig.app.json",
        "assets": [
          "apps/ng-rspack/src/favicon.ico",
          "apps/ng-rspack/src/assets"
        ],
        "styles": ["apps/ng-rspack/src/styles.css"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "outputHashing": "all",
          "extractLicenses": false
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/rspack:dev-server",
      "configurations": {
        "development": {
          "buildTarget": "ng-rspack:build:development"
        }
      },
      "defaultConfiguration": "development"
    }
  }
}
